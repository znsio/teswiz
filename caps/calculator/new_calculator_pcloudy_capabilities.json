{
  "android": {
    "app": "https://github.com/anandbagmar/sampleAppsForNativeMobileAutomation/raw/main/Calculator_8.4.1.apk",
    "appPackage": "com.google.android.calculator",
    "appActivity": "com.android.calculator2.Calculator",
