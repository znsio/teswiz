  "serverConfig" : {
    "server": {
      "port": 31337,
      "use-plugins": ["device-farm"],
      "plugin": {
        "device-farm": {
          "platform": "android",
          "skipChromeDownload": true,
          "cloud": {
            "cloudName": "pCloudy",
