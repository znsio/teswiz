{
  "android": {
    "adbExecTimeout": 45000,
    "app": "https://github.com/anandbagmar/sampleAppsForNativeMobileAutomation/raw/main/ajio-8-3-4.apk",
    "appActivity": "com.ril.ajio.launch.activity.SplashScreenActivity",
    "appPackage": "com.ril.ajio",
    "appiumVersion" : "2.0.0",
    "appWaitDuration": 45000,
    "automationName": "UiAutomator2",
    "autoGrantPermissions": true,
    "deviceName": "android",
    "dontStopAppOnReset": true,
    "enablePerformanceLogging": true,
    "eventTimings": true,
    "newCommandTimeout": 12000,
    "noSign": true,
    "platformName": "android",
    "printPageSourceOnFindFailure": true,
    "skipUnlock": true,
    "noReset": false
  },
  "appiumServerPath": "./node_modules/appium/build/lib/main.js",
  "serverConfig" : {
    "server": {
      "use-plugins": ["device-farm", "relaxed-caps-plugin"],
      "plugin": {
        "device-farm": {
          "platform": "android",
          "skipChromeDownload": true
        }
      }
    }
  }
}