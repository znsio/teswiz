{
  "android": {
    "app": "https://github.com/anandbagmar/sampleAppsForNativeMobileAutomation/raw/main/TheApp-release.apk",
    "appPackage": "com.appiumpro.the_app",
    "appActivity": "com.appiumpro.the_app.MainActivity",
    "automationName": "UIAutomator2",
    "autoGrantPermissions": true,
    "launchTimeout": 90000,
    "newCommandTimeout": 600,
    "pCloudy_DurationInMinutes": 10,
    "platformName": "android",
    "os_version": "13",
    "device": "Samsung",
    "pCloudy_WildNet": "false",
    "pCloudy_EnableVideo": "true",
    "pCloudy_EnablePerformanceData": "true",
    "pCloudy_EnableDeviceLogs": "true",
    "unicodeKeyboard": true,
    "resetKeyboard": true,
    "noReset": false
  },
  "serverConfig": {
    "server": {
      "use-plugins": ["device-farm"],
      "port": 31337,
      "plugin": {
        "device-farm": {
          "platform": "android",
          "cloud":
          {
            "cloudName": "pCloudy",
            "apiUrl": "https://device.pcloudy.com",
            "url": "https://device.pcloudy.com/appiumcloud/wd/hub",
            "devices": [
            ]
          }
        }
      }
    }
  },
  "appiumServerPath": "./node_modules/appium/build/lib/main.js"
}